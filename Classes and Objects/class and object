#include<iostream>
using namespace std;
class student
{
public:
    int id;
    double gpa;
};
int main()
{
    student Alim,Antora;
    Alim.id=101;
    Alim.gpa=3.1516;
    cout<<Alim.id<<"  "<<Alim.gpa<<endl;


    Antora.id=101;
    Antora.gpa=3.1516;
    cout<<Antora id<<"  "<<Antora.gpa<<endl;

}

